newrelic:
  #
  # Update with your New Relic account license key:
  #
  license_key: <%= ENV["NEWRELIC_LICENSE_KEY"] %>
  #
  # Set to '1' for verbose output, remove for normal output.
  # All output goes to stdout/stderr.
  #
  verbose: 1
  #
  # Update with your proxy information (optional) for
  # communicating with new relic servers if you are behind
  # a firewall.
  #
  proxy:
  address: <%= ENV["PROXY_URL"] %>
  port: <%= ENV["PROXY_PORT"] %>
  user: <%= ENV["PROXY_NAME"] %>
  password: <%= ENV["PROXY_PASSWORD"] %>

agents:
  elasticsearch:
    -
      # Application name
      name: <%= ENV["APPLICATION_NAME"] %>

      url: <%= ENV["ELASTICSEARCH_URL"] %>

      # Username and password for HTTP basic auth
      username: <%= ENV["ELASTICSEARCH_USER_NAME"] %>
      password: <%= ENV["ELASTICSEARCH_PASSWORD"] %>

      # Index name
      # using '_all' with provide stats for all indices
      index: '_all'
